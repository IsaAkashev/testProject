{
  "author": "SBT",
  "license": "MIT",
  "name": "@pvm-ui/kit",
  "version": "0.21.4",
  "description": "PVM UIKit разработанный на основе ИДС",
  "repository": {
    "type": "git",
    "url": "https://sberworks.ru/bitbucket-ci/projects/PVM/repos/pvm_uikit"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./components/**/*": {
      "types": "./dist/components/**/*/index.d.ts",
      "import": "./dist/components/**/*/index.js"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build",
    "build": "rimraf ./dist && rslib build",
    "dev": "rslib build --watch",
    "check:tsc": "tsc --project ./tsconfig.json --noEmit",
    "lint": "eslint -c eslint.config.mjs --ext .ts,.tsx ./src",
    "lint:fix": "eslint -c eslint.config.mjs --ext .ts,.tsx ./src --fix",
    "pretty": "prettier --write --ignore-unknown './**/*.{js,jsx,ts,tsx,css,scss,md,mdx,json}' --config ./.prettierrc",
    "pre-commit": "lint-staged",
    "prepublishOnly": "npm run build",
    "prepare": "husky"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run check:tsc"
    }
  },
  "lint-staged": {
    "*.(ts|tsx)": [
      "npm run lint:fix",
      "npm run pretty"
    ],
    "*.css": [
      "npm run pretty"
    ],
    "*.json": [
      "npm run pretty"
    ]
  },
  "peerDependencies": {
    "@sds-eng/base": "1.6.0",
    "react": ">=18.3.1",
    "react-dom": ">=18.3.1"
  },
  "bundleDependencies": [
    "@tanstack/react-table",
    "@tanstack/react-virtual",
    "@dnd-kit/core",
    "@dnd-kit/modifiers",
    "@dnd-kit/sortable",
    "@dnd-kit/utilities"
  ],
  "dependencies": {
    "@dnd-kit/core": "6.3.1",
    "@dnd-kit/modifiers": "9.0.0",
    "@dnd-kit/sortable": "10.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "@sds-eng/base": "1.6.0",
    "@tanstack/react-table": "8.21.3",
    "@tanstack/react-virtual": "3.13.12",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4.1.1",
    "@eslint/compat": "1.3.2",
    "@eslint/js": "10.0.0",
    "@faker-js/faker": "9.9.0",
    "@microsoft/api-extractor": "7.52.11",
    "@rsbuild/core": "1.5.1",
    "@rsbuild/plugin-react": "1.4.0",
    "@rslib/core": "0.12.4",
    "@rspack/core": "1.5.1",
    "@rspack/plugin-react-refresh": "1.5.0",
    "@storybook/addon-docs": "9.1.3",
    "@storybook/react": "9.1.3",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/parser": "8.41.0",
    "css-loader": "7.1.2",
    "dotenv": "17.2.1",
    "eslint": "9.34.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-mdx": "3.6.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "globals": "16.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.5",
    "moment": "2.30.1",
    "postcss-loader": "6.2.1",
    "prettier": "3.6.2",
    "storybook": "9.1.3",
    "storybook-react-rsbuild": "2.0.4",
    "style-loader": "3.3.1",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "typescript": "5.9.2",
    "typescript-eslint": "8.41.0"
  },
  "publishConfig": {
    "registry": "https://sberworks.ru/nexus-ci/repository/sbt_npm/"
  }
}

{
  "name": "mfe-aiom",
  "version": "1.6.0",
  "description": "Panel app with baseline Ver5",
  "scripts": {
    "start": "rspack dev --config rspack.config.dev.ts",
    "build": "rspack build --config rspack.config.prod.ts",
    "lint:fix": "eslint \"src/**/*.{ts,tsx,scss,js}\" --fix",
    "pretty": "prettier --write --ignore-unknown './**/*.{js,jsx,ts,tsx,css,scss,md,mdx,json}' --config ./.prettierrc.js",
    "pretty:json": "prettier --write --ignore-unknown './**/*.json' --config ./.prettierrc.js",
    "update-version": "npx @pvm-ui/pvm-core update-version",
    "prepare": "husky"
  },
  "author": "PVM Indicator",
  "license": "MIT",
  "devDependencies": {
    "@eslint/compat": "1.3.2",
    "@eslint/js": "9.34.0",
    "@rspack/cli": "1.5.0",
    "@rspack/core": "1.5.0",
    "@rspack/plugin-react-refresh": "1.4.3",
    "@types/qs": "6.14.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/parser": "8.40.0",
    "compression-webpack-plugin": "11.1.0",
    "cross-env": "7.0.3",
    "css-loader": "7.1.2",
    "dotenv": "17.2.1",
    "eslint": "9.34.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "globals": "16.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.5",
    "prettier": "3.6.2",
    "react-refresh": "0.17.0",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "typescript": "5.9.2",
    "typescript-eslint": "8.40.0"
  },
  "dependencies": {
    "@emotion/css": "11.13.5",
    "@iconscout/react-unicons": "1.1.5",
    "@module-federation/bridge-react": "0.18.3",
    "@module-federation/enhanced": "0.18.3",
    "@pvm-ui/pvm-core": "1.8.0",
    "@pvm-ui/pvm-navigation": "1.0.0",
    "@reduxjs/toolkit": "1.5.0",
    "@sds-eng/base": "1.6.0",
    "antd": "4.24.16",
    "axios": "1.11.0",
    "moment": "2.30.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-redux": "7.2.0",
    "react-router-dom": "7.8.2",
    "tapable": "2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(ts|tsx)": [
      "npm run lint:fix",
      "npm run pretty"
    ],
    "*.css": [
      "npm run pretty"
    ],
    "*.json": [
      "npm run pretty"
    ]
  },
  "browserslist": [
    "defaults"
  ]
}

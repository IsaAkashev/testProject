    "sass": "1.77.4",
    "sass-loader": "12.6.0",
    "storybook-addon-themes": "6.1.0",
    "style-loader": "3.3.1",
    "ts-loader": "9.4.2",
    "tslib": "2.3.1",
    "typescript": "4.9.5",
    "webpack": "5.94.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.10.0",
    "webpack-remove-empty-scripts": "0.7.3",
    "yarn-audit-fix": "^10.0.7",
    "yarn-audit-html": "^7.3.2" 
  },
  "resolutions": {
    "moment": "2.29.4",
    "**/react": "18.3.1",
    "**/react-dom": "18.3.1"
  }
}
{
  "name": "indicator-app-dashboards-management",
  "version": "1.0.0",
  "description": "App Dashboards Management",
  "scripts": {
    "build": "grafana-toolkit plugin:build --coverage",
    "test": "grafana-toolkit plugin:test --coverage",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "sign": "grafana-toolkit plugin:sign",
    "start": "yarn watch"
  },
  "author": "Indicator Monitor",
  "license": "Apache-2.0",
  "devDependencies": {
    "@grafana/data": "7.4.0",
    "@grafana/runtime": "7.4.0",
    "@grafana/toolkit": "7.4.0",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/react": "^17.0.9",
    "@types/react-router-dom": "^5.3.1",
    "@types/webpack-env": "^1.16.3",
    "emotion": "10.0.27",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint-plugin-react-hooks": "latest",
    "jest-sonar-reporter": "^2.0.0",
    "tslib": "^2.2.0"
  },
  "dependencies": {
    "antd": "4.15.2",
    "react-router-dom": "5.2.0"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "sonar-report.xml",
    "indent": 4
  },
  "jest": {
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/src/$1",
      "^@utils/(.*)$": "<rootDir>/src/utils/$1"
    }
  },
  "resolutions": {
    "async-validator": "4.0.4",
    "moment": "^2.29.1",
    "@babel/runtime": "7.26.10",
    "path-to-regexp": "0.1.10"
  }
}
